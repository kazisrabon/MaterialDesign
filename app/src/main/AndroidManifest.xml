<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iit.materialdesign">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">

        <activity android:name=".FlexibleSpaceWithImageScrollViewActivity"
            android:label="Flexible"
            android:theme="@style/AppTheme.Toolbar">

        </activity>
        <activity
            android:name=".ParallaxToolbarScrollViewActivity"
            android:label="Parallax"
            android:theme="@style/AppTheme.Toolbar"/>
        <activity
            android:name=".StickyHeaderRecyclerViewActivity"
            android:label="Sticky"
            android:theme="@style/AppTheme.Toolbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Favourite"
            android:label="@string/favourite"
            android:theme="@style/AppTheme.Toolbar"/>
        <activity android:name=".Graph"
            android:label="Graph"
            android:theme="@style/AppTheme.Toolbar"/>

        <service android:enabled="true" android:name=".WebData" />
        
    </application>

</manifest>
